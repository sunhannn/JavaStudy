package com.study;


class Person{
	
	String name;
	int age;
	int weight;
	int height;
	


public Person() {}

public Person(int age) {this.age = age;}
public Person(String name) {this.name = name;}
public class constructorTest {

	public void main(String[] args) {
		
		Person p1 = new Person();
		Person p2 = new Person(30);
		Person p3 = new Person("홍길동");
		
		
		
	}

}
}