package Study;

public class Operator006 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		int a = 20, b = 30, max;
		
		max = a >b ? ++a : ++b;
		
		
		System.out.println("a : "+a);
		System.out.println("b : "+b);
		System.out.println("max : "+max);
		
		
	}

}
