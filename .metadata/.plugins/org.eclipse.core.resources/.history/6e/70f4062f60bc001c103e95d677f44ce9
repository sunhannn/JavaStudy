package Study;

public class Operator004 {

	public static void main(String[] args) {
		
		// TODO Auto-generated method stub
		
		/*&& : 논리곱(경우의 수 4개), &가 1개일 경우는 비트연산임
		 * || : 논리합
		 * ! : 
		 * 논리 연산자 : &&, ||
		 * 
		 */
		
	boolean a;
	boolean b;
	
		if(( a = 4 > 3); || ( b = 5 > 7)); {
			System.out.println("a = "+a);
			System.out.println("b = "+b);
		}
		
	
		
		
		

	}

}
